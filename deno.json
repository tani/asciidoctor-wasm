{
  "name": "@gem/asciidoctor-wasm",
  "version": "0.2023.20",
  "exports": "./src/browser.ts",
  "imports": {
    "@bjorn3/browser_wasi_shim": "npm:@bjorn3/browser_wasi_shim@^0.3.0",
    "@ruby/wasm-wasi": "npm:@ruby/wasm-wasi@^2.6.1"
  },
  "tasks": {
    "test": "deno test -A test/*.test.ts",
    "ruby:install": "bundle install",
    "ruby:build": "bundle exec rbwasm build -o asciidoctor.wasm && npx --yes -p wabt wasm-strip asciidoctor.wasm -o asciidoctor.wasm && gzip -f asciidoctor.wasm",
    "docs:build": "cp asciidoctor.wasm.gz docs/asciidoctor.wasm.gz"
  },
  "publish": {
    "include": [
      "src/*.ts",
      "LICENSE",
      "README.md",
      "asciidoctor.wasm"
    ]
  }
}
